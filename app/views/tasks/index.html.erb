
<table>
  <tr>
    <th>Nombre</th>
    <th>WT</th>
    <th>WC</th>
    <th>WR</th>
    <th>Vence</th>
    <!--<th>Author</th>-->
    <th>Estado</th>
    <th>Asignada a</th>
    <th>Accion</th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
  	<% if task.assigned_to_id == current_user.id %>
    	<!--<td><%= link_to task.subject, project_task_path(@project, task) %></td>-->
    	<td><%= task.subject %></td>
    <%else%>
    	<td><%= link_to task.subject, project_task_path(@project, task) %></td>
    <%end%>
    <td><%= task.estimated_time %></td>
    <td><%= task.completed_time %></td>
    <td><%= diferencia(task) %></td>
    <td><%= task.due_date %></td>
    <td><%= task.state %></td>
    <!--<td><%= task.author.try(:email) %></td>-->
    <td><%= task.assigned.try(:email) %></td>
    <td><%= link_to 'Delete', project_task_path(@project, task), confirm: 'Are you sure?', method: :delete, :class => "btn inverse" %></td>
  </tr>
<% end %>
</table>

<br />

